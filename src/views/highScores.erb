      <h1 class="display-5 fw-bold">Hall Of Fame</h1>
      
      
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Score</th>
    </tr>
  </thead>
  <tbody>
    <% counter = 0%>
    <% @highscores.each do |highscore| %>
    <% counter += 1 %>
    <tr>
      <th scope="row"><%=counter%></th>
      <td><%= highscore['User'] %></td>
      <td><%= highscore['Correct']%> / <%= highscore['Total'] %></td>
    </tr>
    <% end %>
  </tbody>
</table>


